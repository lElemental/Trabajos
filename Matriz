
#include <stdio.h>

int main(){
    //Declarando una matriz 2*2
    int matriz[3][3];
    
    //capturar los datos de la matriz desde la entrada estandar
    printf("Ingrese los elementos de la matriz: \n");
    
    for (int i = 0; i<3; i++)
    {
        for (int j = 0; j < 3 ; j++)
        {
            printf("Elemento [%d] [%d] : ", i+1, j+1);
            scanf("%d", &matriz [i][j]);
        }
    }
    //imptrimir la matriz 
    
 
    
    for (int i = 0; i<3; i++)
    {
        //printf("Matriz: \n");
        for (int j = 0; j < 3 ; j++)
        printf( "%d", matriz[i][j]);
        printf("\n");   
        
    }
    printf("\n"); 
    
for (int i = 0; i<3; i++)
    {
        //printf("Matriz: \n");
        for (int j = 0; j < 3 ; j++)
        printf( "%d", matriz[i][j] + matriz[i][j]);
        printf("\n");   
        
    }
    return 0;
}
